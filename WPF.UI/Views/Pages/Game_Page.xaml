<Page x:Class="WPF.UI.Views.Pages.Game_Page"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPF.UI.Views.Pages"   
      xmlns:monoGame="clr-namespace:WPF.UI.MonoGameControls"
      xmlns:monoGameVM="clr-namespace:WPF.UI.ViewModels.Game"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Game_Page"
      xmlns:vm="clr-namespace:WPF.UI.ViewModels.PagesVM"
      d:DataContext="{d:DesignInstance vm:GamePage_ViewModel, IsDesignTimeCreatable=True}">
    <monoGame:MonoGameContentControl>
        <monoGame:MonoGameContentControl.DataContext>
            <monoGameVM:GameViewModel/>
        </monoGame:MonoGameContentControl.DataContext>
    </monoGame:MonoGameContentControl>
</Page>
